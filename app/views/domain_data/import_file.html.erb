<table>
  <thead>
  <tr>
    <th>Date</th>
    <th>Domain rating</th>
    <th>Domain name</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @domain_data.each do |domain_datum| %>
    <tr>
      <td><%= domain_datum.date %></td>
      <td><%= domain_datum.domain_rating %></td>
      <td><%= domain_datum.domain_name %></td>
    </tr>
  <% end %>
  </tbody>
</table>

